<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Invoicing_Mode_to_be_read_only</fullName>
    <active>false</active>
    <errorConditionFormula>AND(!ISNEW() , $Profile.Name &lt;&gt; &quot;System Administrator&quot;,$Profile.Name &lt;&gt; &quot;Customer Onboarding Profile&quot;,$Profile.Name &lt;&gt; &quot;CustomerOnboarding Profile&quot;)</errorConditionFormula>
    <errorDisplayField>Invoicing_Mode__c</errorDisplayField>
    <errorMessage>Invoicing mode cannot be edited.Please contact System Administrator.</errorMessage>
</ValidationRule>
