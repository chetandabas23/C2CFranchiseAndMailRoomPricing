<apex:page standardController="Product2" extensions="C2CProductDetailPageExtension" showHeader="false" standardStylesheets="false" sidebar="false" applyHtmlTag="false" applyBodyTag="false" docType="html-5.0">
    
    <html xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" lang="en">
        <head>
            <meta charset="utf-8" />
            <meta http-equiv="x-ua-compatible" content="ie=edge" />
            <meta name="viewport" content="width=device-width, initial-scale=1" />
            <!-- Import the Design System style sheet -->
            <apex:slds />
            
            <style>
                .minfyTable
                {
                border-collapse: collapse;
                width : 30%;
                }
                .mainTable
                {
                border-collapse: collapse;
                width : 100%;
                }
                /*table {
                border-collapse: collapse;
                width: 100%;
                }*/
                table td {
                
                Text-align:center;
                }
                
                
                #ExpressTable{
                height:100%
                }
                #HyperlocalTable{
                height:100%
                }
                
                #heading {
                background-color: #698abf;
                color: white;
                font-weight: bold;
                padding: 8px;
                letter-spacing: 0.5px;
                font-size:13px;
                }
                p{
                font-size:15px;
                }
                .centerAlign{
                text-align: center !important;
                }
                
                .pbButton {
                text-align:center !important;
                }
