        </head>
        <body>
            <!-- REQUIRED SLDS WRAPPER -->
            <div class="slds-scope">
                
                <apex:form rendered="{!Product2.RecordType.Name=='Hyperlocal'}">
                    <apex:pageBlock ><br/>
                        <apex:pageBlockButtons ><br/>
                            <apex:CommandButton styleClass="slds-button slds-button_brand" value="Back" action="{!goBack}"  />
                            <apex:CommandButton styleClass="slds-button slds-button_brand" value="Edit" action="{!edit}"  rendered="{!$Profile.Name=='System Administrator'}"/>
                        </apex:pageBlockButtons><br/>
                        
                        <apex:outputPanel id="table1">
                            <apex:variable value="{!0}" var="cnt" />
                            <table border="1" id="HyperlocalTable" class="slds-table slds-table_bordered slds-table_cell-buffer mainTable">      
                                <tr border="1">
                                    <td id="heading"><center>No of Order per trip</center></td>
                                    <td id="heading"><center>Base Charge per Order(INR)</center></td>
                                    <td id="heading"><center>Base Range(IN KM)</center></td>
                                    <td id="heading"><center>Additional Charge(INR Per Km)</center></td>
                                    <td id="heading"><center>Additional Range(in Km)</center></td>
                                    <td id="heading"><center>COD Charges(Applicable only onOrders size > INR 100)[%]</center></td>
                                    <td id="heading"><center>COD Charges(Applicable only on Orders size > INR 100)[INR]</center></td>
                                </tr>
                                <apex:repeat value="{!hpList}" var="hp">   
                                    <tr border="1">
                                        <td><center>
                                            <apex:outputField styleClass="slds-output" value="{!hp.Number_of_Order_per_trip__c}"  />
                                            </center></td>
                                        <td><center>
                                            <apex:outputField styleClass="slds-output" value="{!hp.Base_Charge_per_Order_INR__c}"/>
                                            </center></td>
                                        <td><center>
                                            <apex:outputField styleClass="slds-output" value="{!hp.Base_Range_in_Km__c}"  />
                                            </center></td>
                                        <td><center>
                                            <apex:outputField styleClass="slds-output" value="{!hp.Additional_Charge_INR_per_Km__c}"  />
                                            </center></td>
                                        <td><center>
                                            <apex:outputField styleClass="slds-output" value="{!hp.Additional_Range__c}"  />
                                            </center>
                                        </td>
                                        <td><center>
                                            <apex:outputField styleClass="slds-output" value="{!hp.COD_Charges_in__c}"  />
                                            </center></td>
                                        <td><center>
                                            <apex:outputField styleClass="slds-output" value="{!hp.COD_Charges__c}"  />
                                            </center></td> 
                                    </tr>
                                </apex:repeat>                                              
                            </table>
                        </apex:outputPanel>
                        <apex:pageBlockSection >
                        </apex:pageBlockSection>
                    </apex:pageBlock>
                </apex:form>
                
                
                <apex:form rendered="{!Product2.RecordType.DeveloperName == 'Express_shipping' }" >
                    <apex:pageBlock id="Express">
                        
                        <apex:pageBlockButtons >
                            
                            <apex:CommandButton styleClass="slds-button slds-button_brand" value="Back" action="{!goBack}"  />       
                            <apex:CommandButton styleClass="slds-button slds-button_brand" value="Edit" action="{!edit}"  rendered="{!$Profile.Name=='System Administrator'}"/>
                        </apex:pageBlockButtons><br/>
                        <apex:pageMessages ></apex:pageMessages>
                        <table cellspacing="10" cellpadding="10" border="1" id="ExpressTable" class="slds-table slds-table_bordered slds-table_cell-buffer mainTable">
                            <tr><td colspan="8" id="heading" ><center><p>Delhivery Commercials - EXPRESS</p></center></td></tr> 
                            <tr>
                                <th rowspan="2" id="heading"><center>Weight Slabs</center></th>
                                <th id="heading" ><center>Within-city</center></th>
                                <th id="heading" > <center>Regional (Single <br/>Connection And <br/> Less than 500 Kms)</center></th>
                                <th id="heading" ><center> Metro-Metro </center></th>
                                <th id="heading"><center> Rest of India </center></th>
                                <th id="heading"><center>North East, HP,<br/> Port Blair,J And K </center></th>
                            </tr>
                            <tr>
                                <th id="heading"><center>Zone A</center></th>
                                <th id="heading"><center>Zone B </center></th>
                                <th id="heading"><center>Zone C</center></th>
                                <th id="heading"><center>Zone D</center></th>
                                <th id="heading"><center>Zone E</center></th>
                            </tr>
                            <tr>
                                <th id="heading">Upto 500 gms</th>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_A_Upto_500_gms__c}"  /> </td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_B_Upto_500_gms__c}"   /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_C_Upto_500_gms__c}"   /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_D_Upto_500_gms__c}"   /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_E_Upto_500_gms__c}"   /></td>
                            </tr>
                            <tr>
                                <th id="heading">Every additional 500 gms</th>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_A_Every_additional_500_gms__c}"   /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_B_Every_additional_500_gms__c}"   /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_C_Every_additional_500_gms__c}"    /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_D_Every_additional_500_gms__c}"    /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_E_Every_additional_500_gms__c}"    /></td>
                            </tr>
                            <tr>
                                <th id="heading">RTO (Every 500 gms)</th>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_A_RTO_every_500_gms__c}"   /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_B_RTO_every_500_gms__c}"   /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_C_RTO_every_500_gms__c}"   /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_D_RTO_every_500_gms__c}"   /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_E_RTO_every_500_gms__c}"   /></td>
                            </tr>
                            <tr>
                                <th id="heading" >DTO Upto 500 gm</th>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_A_DTO_Upto_500gm__c}"   /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_B_DTO_Upto_500gm__c}"   /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_C_DTO_Upto_500gm__c}"   /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_D_DTO_Upto_500gm__c}"   /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_E_DTO_Upto_500gm__c}"   /></td>
                            </tr>
                            <tr>
                                <th id="heading" >DTO Every Add 500 gm</th>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_A_DTO_every_add_500gm__c}"   /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_B_DTO_every_add_500gm__c}"   /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_C_DTO_every_add_500gm__c}"   /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_D_DTO_every_add_500gm__c}"   /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_E_DTO_every_add_500gm__c}"   /></td>
                            </tr>
                            <tr>
                                <th id="heading">COD Charges</th>
                                <!-- <apex:outputPanel rendered="{!objectOLI.COD_Charges__c != null && objectOLI.COD_Value__c != null}"> -->
                                <apex:outputPanel rendered="{!objProduct.COD_Charges__c != null || objProduct.COD_Value__c != null }"> 
                                    <td colspan="5"><center>
                                        <apex:outputField styleClass="slds-output" value="{!objProduct.COD_Charges__c}"  />
                                        <apex:outputPanel rendered="{!objProduct.COD_Value__c != null}">
                                            
                                            <apex:outputPanel rendered="{!objProduct.COD_Value__c != null && objProduct.COD_Charges__c !=null}">
                                                or&nbsp;
                                            </apex:outputPanel>
                                            
                                            <apex:outputField styleClass="slds-output" value="{!objProduct.COD_Value__c}"  />
                                            of product bill value
                                            
                                            <apex:outputPanel rendered="{!objProduct.COD_Value__c != null && objProduct.COD_Charges__c !=null}">
                                                whichever is higher 
                                            </apex:outputPanel>
                                            
                                        </apex:outputPanel>
                                        </center>
                                    </td>
                                </apex:outputPanel>
                                <apex:outputPanel rendered="{!objProduct.COD_Charges__c == null && objProduct.COD_Value__c == null}">
                                    <td colspan="5"/>
                                </apex:outputPanel>
                            </tr>
                            
                            
                            <tr><!-- For Wallet Delivery Charges -->
                                <apex:outputPanel rendered="{!objProduct.Wallet_Transaction_Charge__c != null || objProduct.Wallet_Transaction_Rate__c != null }"> 
                                    <th id="heading">Wallet on Delivery Charges (Paytm)</th>
                                    <td colspan="5">
                                        <center>
                                            Per Applicable shipment Transaction Charge of the flat Wallet charges of &nbsp;
                                            <apex:outputField styleClass="slds-output" value="{!objProduct.Wallet_Transaction_Charge__c}"  />
                                            
                                            <apex:outputPanel rendered="{!objProduct.Wallet_Transaction_Rate__c != null && objProduct.Wallet_Transaction_Charge__c !=null}">
                                                +&nbsp;
                                                <apex:outputField styleClass="slds-output" value="{!objProduct.Wallet_Transaction_Rate__c}"  />
                                                &nbsp;of Invoice Value
                                            </apex:outputPanel>
                                        </center>
                                    </td>
                                </apex:outputPanel>
                            </tr>
                            
                            
                            <tr><!-- For Card on Delivery Charges -->
                                <apex:outputPanel rendered="{!objProduct.Transaction_Charge__c != null || objProduct.Credit_Card_Transaction_Charge__c != null }"> 
                                    <th id="heading">Card on Delivery Charges</th>
                                    
                                    <td colspan="2">
                                         MDR rates + Flat transaction charge for<br/>
                                        <apex:outputPanel rendered="{!objProduct.Transaction_Charge__c != null}">
                                            Debit Card will be&nbsp;
                                            <apex:outputField styleClass="slds-output" value="{!objProduct.Transaction_Charge__c}"  /> 
                                        </apex:outputPanel>
                                        <apex:outputText rendered="{!IF(objProduct.Transaction_Charge__c != null && objProduct.Credit_Card_Transaction_Charge__c != null,true,false)}" value=" and " />
                                        <apex:outputPanel rendered="{!objProduct.Credit_Card_Transaction_Charge__c != null}">
                                            Credit Card will be&nbsp;
                                            <apex:outputField styleClass="slds-output" value="{!objProduct.Credit_Card_Transaction_Charge__c}"  />
                                        </apex:outputPanel>
                                    </td>
                                    <td colspan="3">
                                        Where MDR rates are as follows: <br/>
                                        <apex:outputPanel rendered="{!objProduct.Credit_Card_Rates__c != null}">
                                            1. Credit Card rates:&nbsp;<apex:outputField styleClass="slds-output" value="{!objProduct.Credit_Card_Rates__c}" />&nbsp;(Total Invoice Value)<br/>
                                        </apex:outputPanel>
                                        <apex:outputtext value="{!IF(objProduct.Credit_Card_Rates__c == null,'1.','2.')}" />&nbsp;Debit card rates: Transaction Amount up to&nbsp;<apex:outputField styleClass="slds-output" value="{!objProduct.Threshold_Charge__c}" /> -&nbsp;<apex:outputText styleClass="slds-output" value="{!IF(objProduct.Debit_Before_Rates__c == 0,'Free','')}" />  <apex:outputField styleClass="slds-output" rendered="{!objProduct.Debit_Before_Rates__c != 0} " value="{!objProduct.Debit_Before_Rates__c}" /> <br/>
                                        ><apex:outputField styleClass="slds-output" value="{!objProduct.Threshold_Charge__c}" /> -&nbsp;<apex:outputField styleClass="slds-output" value="{!objProduct.Debit_After_Rates__c}" />&nbsp;of Transaction Amount<br/>
                                    </td>
                                </apex:outputPanel>
                            </tr>
                            
                            <tr><!-- For FOV Factor -->
                                <apex:outputPanel rendered="{!objProduct.Frieght_Value_Factor__c != null}" >
                                <th id="heading">FOV Factor(Frieght On Value)</th>
                                <td colspan="5">
                                    <center>
                                        <apex:outPutText rendered="{!objProduct.Frieght_Value_Factor__c == 0 || objProduct.Frieght_Value_Factor__c == null}" value="Default to be 0" /> 
                                        <apex:outputField rendered="{!objProduct.Frieght_Value_Factor__c != 0 && objProduct.Frieght_Value_Factor__c != null}" value="{!objProduct.Frieght_Value_Factor__c}"   />
                                    </center>
                                </td>
                                </apex:outputPanel>
                            </tr>
                            
                            
                            
                            
                            <tr>
                                <th id="heading">Volumetric Divisor</th>
                                <td colspan="5"><apex:outputText styleClass="slds-output" value="{!objProduct.Volumetric_Formulae__c}" /></td>
                            </tr>
                            <tr>
                                <th id="heading">GST</th>
                                <td colspan="5">As Applicable</td>
                            </tr>
                            <tr>
                                <th id="heading">Fuel Surcharge (FSC)</th>
                                
                                <apex:outputPanel rendered="{!objProduct.Fuel_Surcharge__c!=null }">
                                    <td colspan="5"><center><apex:outputField styleClass="slds-output" value="{!objProduct.Fuel_Surcharge__c}"  />&nbsp;on Freight Charges</center></td>
                                </apex:outputPanel>
                                
                                <apex:outputPanel rendered="{!objProduct.Fuel_Surcharge__c==null }">
                                    <td colspan="5"/>
                                </apex:outputPanel>
                                
                            </tr>
                            <tr>
                                <th id="heading">Maximum Liability for  Forward (INR)</th>
                                <td colspan="5"><apex:outputField styleClass="slds-output" value="{!objProduct.Maximum_liability_per_consignment__c}"  /></td>
                            </tr>
                            
                            <tr>
                                <th id="heading">Maximum Liability for Reverse </th>
                                <apex:outputPanel rendered="{!objProduct.Maximum_Liability_for_Reverse__c != null || objProduct.Maximum_Liability_for_Reverse_Percent__c != null }"> 
                                    <td colspan="5"><center>
                                        <!-- When Both % and Value is Present -->
                                        <apex:outputPanel rendered="{!objProduct.Maximum_Liability_for_Reverse_Percent__c != null && objProduct.Maximum_Liability_for_Reverse__c !=null}">
                                            Lower of &nbsp;<apex:outputField styleClass="slds-output" value="{!objProduct.Maximum_Liability_for_Reverse_Percent__c}"  />
                                            of product value or &nbsp;<apex:outputField value="{!objProduct.Maximum_Liability_for_Reverse__c}"  />
                                        </apex:outputPanel>
                                        <!-- When Only Value is Present -->
                                        <apex:outputPanel rendered="{!objProduct.Maximum_Liability_for_Reverse__c != null && objProduct.Maximum_Liability_for_Reverse_Percent__c == null}">
                                            <apex:outputField styleClass="slds-output" value="{!objProduct.Maximum_Liability_for_Reverse__c}"  />
                                        </apex:outputPanel>      
                                        <!-- When Only % is Present -->        
                                        <apex:outputPanel rendered="{!objProduct.Maximum_Liability_for_Reverse_Percent__c != null && objProduct.Maximum_Liability_for_Reverse__c ==null}">
                                            Lower of &nbsp;<apex:outputField styleClass="slds-output" value="{!objProduct.Maximum_Liability_for_Reverse_Percent__c}"  /> of product value
                                        </apex:outputPanel>
                                        </center>
                                    </td>
                                </apex:outputPanel>
                                <!-- Display Blank when both are undefined -->
                                <apex:outputPanel rendered="{!objProduct.COD_Charges__c == null && objProduct.COD_Value__c == null}">
                                    <td colspan="5"/>
                                </apex:outputPanel>
                                
                            </tr>
                            
                            <tr>
                                <th id="heading">COD Remittance</th>
                                <td colspan="8">
                                    <apex:outputLabel styleClass="slds-output" rendered="{!objProduct.COD_Remittance__c=='Weekly'}">
                                        {!objProduct.COD_Remittance__c}(Fri)
                                    </apex:outputLabel>
                                    <apex:outputLabel styleClass="slds-output" rendered="{!objProduct.COD_Remittance__c=='Bi-weekly'}">
                                        {!objProduct.COD_Remittance__c}(Tue,Fri)
                                    </apex:outputLabel>
                                    <apex:outputLabel styleClass="slds-output" rendered="{!objProduct.COD_Remittance__c=='Tri-weekly'}">
                                        {!objProduct.COD_Remittance__c}(Mon,Wed,Fri)
                                    </apex:outputLabel>
                                    <apex:outputLabel styleClass="slds-output" rendered="{!objProduct.COD_Remittance__c=='Daily'}">
                                        {!objProduct.COD_Remittance__c}(Mon,Tue,Wed,Thu,Fri)
                                    </apex:outputLabel>
                                </td>
                            </tr>
                        </table>
                        
                    </apex:pageBlock>
                </apex:form>
                
                <apex:form rendered="{!Product2.RecordType.DeveloperName == 'Standard_Shipping' }" >
                    <apex:pageBlock >
                        <apex:pageMessages ></apex:pageMessages>
                        <apex:pageBlockButtons >
                            <apex:CommandButton styleClass="slds-button slds-button_brand" value="Back" action="{!goBack}"  />
                            <apex:CommandButton styleClass="slds-button slds-button_brand" value="Edit" action="{!edit}"   rendered="{!$Profile.Name=='System Administrator'}"/>
                        </apex:pageBlockButtons><br/>
                        <table cellspacing="10" cellpadding="10" border="1" class="slds-table slds-table_bordered slds-table_cell-buffer mainTable">
                            <tr><td colspan="8" id="heading" ><center><p>Delhivery Commercials - STANDARD</p></center></td></tr> 
                            <tr>
                                <th rowspan="2" id="heading"><center>Weight Slabs</center></th>
                                <th id="heading" ><center>Within City</center></th>
                                <th id="heading" ><center>Regional (Single <br/>Connection And <br/>Less than 500 Kms)</center></th>
                                <th colspan ="2" id="heading"><center>Metro to Metro<br/>(ZONE C)</center></th>
                                <th colspan="2" id="heading" ><center>Rest of India <br/>(ZONE D)</center></th>
                                <th id="heading" ><center>North East,HP,<br/> Port Blair, J And K</center></th>
                            </tr>
                            <tr><td id="heading"><center>ZONE A</center></td><td id="heading"><center>ZONE B</center></td><td id="heading" ><center>N1</center></td><td id="heading"><center >N2</center></td><td id="heading"><center>N1</center></td><td id="heading"><center>N2</center></td><td id="heading"><center>ZONE E</center></td></tr>
                            
                            <tr>
                                <th id="heading">Upto 500 gms</th>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_A_Upto_500_gms__c}"  /> </td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_B_Upto_500_gms__c}"   /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_C_N1_Upto_500_gms__c}"   /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_C_N2_Upto_500_gms__c}"   /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_D_N1_Upto_500_gms__c}"   /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_D_N2_Upto_500_gms__c}"   /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_E_Upto_500_gms__c}"   /></td>
                            </tr>
                            
                            <tr>
                                <th id="heading" >Every Additional 500 gms(Upto 3 kgs)</th>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_A_Every_Add_500_gms_upto_3_kgs__c}"   /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_B_Every_Add_500_gms_upto_3_kgs__c}"   /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_C_N1_Every_Add_500_gms_Upto_3_kgs__c}"    /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_C_N2_Every_Add_500_gms_Upto_3_kgs__c}"    /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_D_N1_Every_Add_500_gms_Upto_3_kgs__c}"    /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_D_N2_Every_Add_500_gms_Upto_3_kgs__c}"    /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_E_Every_additional_500_gms__c}"    /></td>
                            </tr>
                            <tr>
                                <th id="heading" >Every Additional Slab (1 Kg)</th>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_A_Every_Additional_Slab_1_Kg__c}"   /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_B_Every_Additional_Slab_1_Kg__c}"   /></td>
                                <td><apex:outputField styleClass="slds-output" value="{!objProduct.Zone_C_N1_Every_Additional_Slab_1_Kg__c}"   /></td>
