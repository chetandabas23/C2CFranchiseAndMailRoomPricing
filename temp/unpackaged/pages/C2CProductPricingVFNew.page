<apex:page standardController="Opportunity" extensions="C2CProductPricingVFExtension" id="thePage" standardStylesheets="false" readOnly="true"> 
    <html>
        <head>
            <style>
                table {
                border-collapse: collapse;
                width: 100%;
                }
                table td {
                Text-align:center;
                }
                
                #Table{
                height:100%
                }
                #HyperlocalTable{
                height:100%
                }
                
                #heading {
                background-color: #698abf;
                color: white;
                font-weight: bold;
                padding: 8px;
                letter-spacing: 0.5px;
                font-size:13px;
                text-align: left;
                }
                p{
                font-size:15px;
                }
                li{width: 520px;
                float: right;
                
                }ul{
                text-align: left;
                }
                .CustomExpress{
                
                height: 725px;
                width: 999px;
                }
            </style>
            <!-- <script>
function addINR(Fieldvalue){
    var field=Fieldvalue;
    if( field == null){
        return 0.00;
    }
    else{
        {!objectOLI.Zone_A_Upto_500_gms__c};
    }
}
</script> -->   
        </head>
        <body>
            <apex:form rendered="{!opp.OpportunityLineItems.size == 0}" >
                <center><p style="height:100px;width:800px;color:white; background-color: #698abf;">
                    <br/><br/>
                    There is no pricing selected for this Opportunity. To create, please &nbsp;
                    <apex:outputLink target="_top" rendered="{!opp.RecordType.Name!='C2C'}" value="{!URLFOR('/apex/ProductPricingEditableVFNew?scontrolCaching=1&id='+Opportunity.Id)}">Click Here</apex:outputLink>
                    <apex:outputLink target="_top" rendered="{!opp.RecordType.Name=='C2C'}" value="{!URLFOR('/apex/FranchisePricing?scontrolCaching=1&id='+Opportunity.Id)}">Click Here</apex:outputLink>
                    </p></center>
            </apex:form>
            
            <apex:form rendered="{!IF(AND (Opportunity.RecordType.Name=='Hyperlocal',opp.OpportunityLineItems.size>0,hpList.size>0), true ,false )}">
                <apex:pageblock >
                    <apex:outputPanel id="table1" rendered="{!opp.OpportunityLineItems.size>0}">
                        <apex:variable value="{!0}" var="cnt" />
                        <table cellspacing="10" cellpadding="10" border="1" id="HyperlocalTable">      
                            <tr border="1">       
                                <td id="heading"><center>No of Order per trip</center></td>
                                <td id="heading"><center>Base Charge per Order(INR)</center></td>
                                <td id="heading"><center>Base Range(IN KM)</center></td>
                                <td id="heading"><center>Additional Charge(INR Per Km)</center></td>
                                <td id="heading"><center>Additional Range(in Km)</center></td>
                                <td id="heading"><center>COD Charges(Applicable only on<br/> Orders size > INR 100)[%]</center></td>
                                <td id="heading"><center>COD Charges(Applicable only on<br/> Orders size > INR 100)[INR]</center></td>
                            </tr>
                            
                            <apex:repeat value="{!hpList}" var="hp">   
                                <tr border="1">
                                    
                                    <td><center>
                                        <apex:outputField value="{!hp.Number_of_Order_per_trip__c}"/>
                                        </center>
                                    </td>
                                    <td><center>
                                        <apex:outputField value="{!hp.Base_Charge_per_Order_INR__c}"/>
                                        </center>
                                    </td>
                                    <td><center>
                                        <apex:outputField value="{!hp.Base_Range_in_Km__c}"/>
                                        </center>
                                    </td>
                                    <td><center>
                                        <apex:outputField value="{!hp.Additional_Charge_INR_per_Km__c}"/>
                                        </center>
                                    </td>
                                    <td><center>
                                        <apex:outputField value="{!hp.Additional_Range__c}"/>
                                        </center>
                                    </td>
                                    <td><center>
                                        <apex:outputField value="{!hp.COD_Charges_in__c}"/>
                                        </center>
                                    </td>
                                    <td><center>
                                        <apex:outputField value="{!hp.COD_Charges__c}"/>
                                        </center>
                                    </td> 
                                </tr>
                            </apex:repeat>                                              
                        </table>
                        
                        <apex:pageBlockSection >
                            <apex:pageBlockSectionItem >
                                <apex:repeat value="{!opp.OpportunityLineItems}" var="oli">
                                    <apex:outputField value="{!oli.Description__c}" ></apex:outputField>
                                </apex:repeat>
                            </apex:pageBlockSectionItem> 
                        </apex:pageBlockSection>
                    </apex:outputPanel>
                </apex:pageblock> 
            </apex:form>
            
            
            <apex:form rendered="{!IF(AND(Opportunity.RecordType.DeveloperName=='Express_Shipping',opp.OpportunityLineItems.size>0,!opp.OpportunityLineItems[0].isCustom__c),true,false )}">
                <apex:pageBlock id="Express">
                    <apex:pageMessages ></apex:pageMessages>
                    <table cellspacing="10" cellpadding="10" border="1" id="ExpressTable" >
                        <tr><td colspan="8" id="heading" ><center><p>Delhivery Commercials - EXPRESS</p></center></td></tr> 
                        
                        <tr>
                            <th rowspan="2" id="heading"><center>Weight Slabs</center></th>
                            <th id="heading" ><center>Within-city</center></th>
                            <th id="heading" > <center>Regional (Single <br/>Connection And <br/> Less than 500 Kms)</center></th>
                            <th id="heading" ><center> Metro-Metro </center></th>
                            <th id="heading"><center> Rest of India </center></th>
                            <th id="heading"><center>North East, HP,<br/> Port Blair,J And K </center></th>
                        </tr>
                        
                        <tr>
                            
                            <th id="heading"><center>ZONE A</center></th>
                            <th id="heading"><center>ZONE B </center></th>
                            <th id="heading"><center>ZONE C</center></th>
                            <th id="heading"><center>ZONE D</center></th>
                            <th id="heading"><center>ZONE E</center></th>
                        </tr>
                        
                        <tr>
                            <th id="heading">Upto 500 gms</th>
                            
                            <td><apex:outputField value="{!objectOLI.Zone_A_Upto_500_gms__c}"   /></td> 
                            <td><apex:outputField value="{!objectOLI.Zone_B_Upto_500_gms__c}"   /></td>
                            <td><apex:outputField value="{!objectOLI.Zone_C_Upto_500_gms__c}"   /></td>
                            <td><apex:outputField value="{!objectOLI.Zone_D_Upto_500_gms__c}"   /></td>
                            <td><apex:outputField value="{!objectOLI.Zone_E_Upto_500_gms__c}"   /></td>
                        </tr>
                        
                        <tr>
                            <th id="heading">Every additional 500 gms</th>
                            <td><apex:outputField value="{!objectOLI.Zone_A_Every_additional_500_gms__c}"   /></td>
                            <td><apex:outputField value="{!objectOLI.Zone_B_Every_additional_500_gms__c}"   /></td>
                            <td><apex:outputField value="{!objectOLI.Zone_C_Every_additional_500_gms__c}"    /></td>
                            <td><apex:outputField value="{!objectOLI.Zone_D_Every_additional_500_gms__c}"    /></td>
                            <td><apex:outputField value="{!objectOLI.Zone_E_Every_additional_500_gms__c}"    /></td>
                        </tr>
                        
                        <tr>
                            <th id="heading">RTO (Every 500 gms)</th>
                            <td><apex:outputField value="{!objectOLI.Zone_A_RTO_every_500_gms__c}"   /></td>
                            <td><apex:outputField value="{!objectOLI.Zone_B_RTO_every_500_gms__c}"   /></td>
                            <td><apex:outputField value="{!objectOLI.Zone_C_RTO_every_500_gms__c}"   /></td>
                            <td><apex:outputField value="{!objectOLI.Zone_D_RTO_every_500_gms__c}"   /></td>
                            <td><apex:outputField value="{!objectOLI.Zone_E_RTO_every_500_gms__c}"   /></td>
                        </tr>
                        
                        <tr>
                            <th id="heading" >DTO Upto 500 gm</th>
                            <td><apex:outputField value="{!objectOLI.Zone_A_DTO_Upto_500gm__c}"   /></td>
                            <td><apex:outputField value="{!objectOLI.Zone_B_DTO_Upto_500gm__c}"   /></td>
                            <td><apex:outputField value="{!objectOLI.Zone_C_DTO_Upto_500gm__c}"   /></td>
                            <td><apex:outputField value="{!objectOLI.Zone_D_DTO_Upto_500gm__c}"   /></td>
                            <td><apex:outputField value="{!objectOLI.Zone_E_DTO_Upto_500gm__c}"   /></td>
                        </tr>
                        
                        <tr>
                            <th id="heading" >DTO Every Add 500 gm</th>
                            <td><apex:outputField value="{!objectOLI.Zone_A_DTO_every_add_500gm__c}"   /></td>
                            <td><apex:outputField value="{!objectOLI.Zone_B_DTO_every_add_500gm__c}"   /></td>
                            <td><apex:outputField value="{!objectOLI.Zone_C_DTO_every_add_500gm__c}"   /></td>
                            <td><apex:outputField value="{!objectOLI.Zone_D_DTO_every_add_500gm__c}"   /></td>
                            <td><apex:outputField value="{!objectOLI.Zone_E_DTO_every_add_500gm__c}"   /></td>
                        </tr>
                        
                        <tr>
                            <th id="heading">COD Charges</th>
                            <!-- <apex:outputPanel rendered="{!objectOLI.COD_Charges__c != null && objectOLI.COD_Value__c != null}"> -->
                            <apex:outputPanel rendered="{!objectOLI.COD_Charges__c != null || objectOLI.COD_Value__c != null }"> 
                                <td colspan="5"><center>
                                    <apex:outputField value="{!objectOLI.COD_Charges__c}"  />
                                    <apex:outputPanel rendered="{!objectOLI.COD_Value__c != null}">
                                        
                                        <apex:outputPanel rendered="{!objectOLI.COD_Value__c != null && objectOLI.COD_Charges__c !=null}">
                                            or&nbsp;
                                        </apex:outputPanel>
                                        
                                        <apex:outputField value="{!objectOLI.COD_Value__c}"  />
                                        of product bill value
                                        
                                        <apex:outputPanel rendered="{!objectOLI.COD_Value__c != null && objectOLI.COD_Charges__c !=null}">
                                            whichever is higher 
                                        </apex:outputPanel>
                                        
                                    </apex:outputPanel>
                                    </center>
                                </td>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!objectOLI.COD_Charges__c == null && objectOLI.COD_Value__c == null}">
                                <td colspan="5"/>
                            </apex:outputPanel>
                            
                        </tr>
                        
                        <tr><!-- For Wallet Delivery Charges -->
                            <apex:outputPanel rendered="{!objectOLI.Wallet_Transaction_Charge__c != null || objectOLI.Wallet_Transaction_Rate__c != null }"> 
                                <th id="heading">Wallet on Delivery Charges (Paytm)</th>
                                
                                <td colspan="5">
                                    <center>
                                        Per Applicable shipment Transaction Charge of the flat Wallet charges of &nbsp;
                                        <apex:outputField styleClass="slds-output" value="{!objectOLI.Wallet_Transaction_Charge__c}"  />
                                        
                                        <apex:outputPanel rendered="{!objectOLI.Wallet_Transaction_Rate__c != null && objectOLI.Wallet_Transaction_Charge__c !=null}">
                                            +&nbsp;
                                            <apex:outputField styleClass="slds-output" value="{!objectOLI.Wallet_Transaction_Rate__c}"  />
                                            &nbsp;of Invoice Value
                                        </apex:outputPanel>
                                        
                                    </center>
                                </td>
                            </apex:outputPanel>
                        </tr>
                        
                        
                        <tr><!-- For Card on Delivery Charges -->
                            <apex:outputPanel rendered="{!objectOLI.Transaction_Charge__c != null || objectOLI.Credit_Card_Transaction_Charge__c != null }"> 
                                <th id="heading">Card on Delivery Charges</th>
                                
                                <td colspan="2">
                                    MDR rates + Flat transaction charge for<br/>
                                    <apex:outputPanel rendered="{!objectOLI.Transaction_Charge__c != null}">
                                        Debit Card will be&nbsp;
                                        <apex:outputField styleClass="slds-output" value="{!objectOLI.Transaction_Charge__c}"  /> 
                                    </apex:outputPanel>
                                    <apex:outputText rendered="{!IF(objectOLI.Transaction_Charge__c != null && objectOLI.Credit_Card_Transaction_Charge__c != null,true,false)}" value=" and " />
                                    <apex:outputPanel rendered="{!objectOLI.Credit_Card_Transaction_Charge__c != null}">
                                        Credit Card will be&nbsp;
                                        <apex:outputField styleClass="slds-output" value="{!objectOLI.Credit_Card_Transaction_Charge__c}"  />
                                    </apex:outputPanel>
                                </td>
                                <td colspan="3">
                                    Where MDR rates are as follows: <br/>
                                    <apex:outputPanel rendered="{!objectOLI.Credit_Card_Rates__c != null}">
                                        1. Credit Card rates:&nbsp;<apex:outputField styleClass="slds-output" value="{!objectOLI.Credit_Card_Rates__c}" />&nbsp;(Total Invoice Value)<br/>
                                    </apex:outputPanel>
                                    <apex:outputtext value="{!IF(objectOLI.Credit_Card_Rates__c == null,'1.','2.')}" />&nbsp;Debit card rates: Transaction Amount up to&nbsp;<apex:outputField styleClass="slds-output" value="{!objectOLI.Threshold_Charge__c}" /> -&nbsp;<apex:outputText styleClass="slds-output" value="{!IF(objectOLI.Debit_Before_Rates__c == 0,'Free','')}" />  <apex:outputField styleClass="slds-output" rendered="{!objectOLI.Debit_Before_Rates__c != 0}" value="{!objectOLI.Debit_Before_Rates__c}" /> <br/>
                                    ><apex:outputField styleClass="slds-output" value="{!objectOLI.Threshold_Charge__c}" /> -&nbsp;<apex:outputField styleClass="slds-output" value="{!objectOLI.Debit_After_Rates__c}" />&nbsp;of Transaction Amount<br/>
                                </td>
                            </apex:outputPanel>
                        </tr>
                        
                        <tr><!-- For FOV Factor -->
                            <apex:outputPanel rendered="{!objectOLI.Frieght_Value_Factor__c != null}" >
                            <th id="heading">FOV Factor(Frieght On Value)</th>
                            <td colspan="5">
                                <center>
                                    <apex:outPutText rendered="{!objectOLI.Frieght_Value_Factor__c == 0 || objectOLI.Frieght_Value_Factor__c == null}" value="Default to be 0" /> 
                                    <apex:outputField rendered="{!objectOLI.Frieght_Value_Factor__c != 0 && objectOLI.Frieght_Value_Factor__c != null}" value="{!objectOLI.Frieght_Value_Factor__c}"   />
                                </center>
                            </td>
                            </apex:outputPanel>
                        </tr>
                        
                        
                        <!-- <tr>
<th id="heading">Volumetric Divisor</th>
<td colspan="5"><center><apex:outputText value="{!objectOLI.Volumetric_Formulae__c}" /></center></td>
</tr> -->
                        
                        <tr>
                            <th id="heading">Volumetric Formulae</th>
                            <td colspan="5"><center> L*B*H / (<apex:outputText value="{!objectOLI.Volumetric_Formulae__c}"/>)  L, B, H in cms </center></td>
                        </tr>
                        
                        
                        <!-- <tr>
<th id="heading">GST</th>
<td colspan="5"><center><apex:outputField value="{!objectOLI.Service_Charges__c}"  /></center></td>
</tr> -->
                        
                        <tr>
                            <th id="heading">GST</th>
                            <td colspan="5"><center>As Applicable</center></td>
                        </tr>
                        
                        <tr>
                            <th id="heading">Fuel Surcharge (FSC)</th>
                            <apex:outputPanel rendered="{!objectOLI.Fuel_Surcharge__c!=null }">
                                <td colspan="5"><center><apex:outputField value="{!objectOLI.Fuel_Surcharge__c}"  />&nbsp;on Freight Charges</center></td>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!objectOLI.Fuel_Surcharge__c==null }">
                                <td colspan="5"/>
                            </apex:outputPanel>
                        </tr>
                        
                        <!--   <tr>
<th id="heading">Rural Pin Code Pricing</th>
<td colspan="5"><center><apex:outputField value="{!objectOLI.Rural_Pin_Code_Pricing__c}"  /></center></td>
</tr>-->
                        
                        
                        <tr>
                            <th id="heading">Maximum Liability for Forward</th>
                            <td colspan="5"><center><apex:outputField value="{!objectOLI.Maximum_liability_per_consignment__c}"  /></center></td>
                        </tr> 
                        
                        
                        <tr>
                            <th id="heading">Maximum Liability for Reverse </th>
                            <apex:outputPanel rendered="{!objectOLI.Maximum_Liability_for_Reverse__c != null || objectOLI.Maximum_Liability_for_Reverse_Percent__c != null }"> 
                                <td colspan="5"><center>
                                    <!-- When Both % and Value is Present -->
                                    <apex:outputPanel rendered="{!objectOLI.Maximum_Liability_for_Reverse_Percent__c != null && objectOLI.Maximum_Liability_for_Reverse__c !=null}">
                                        Lower of &nbsp;<apex:outputField value="{!objectOLI.Maximum_Liability_for_Reverse_Percent__c}"  />
                                        of product value or &nbsp;<apex:outputField value="{!objectOLI.Maximum_Liability_for_Reverse__c}"  />
                                    </apex:outputPanel>
                                    
                                    <!-- When Only Value is Present -->
                                    <apex:outputPanel rendered="{!objectOLI.Maximum_Liability_for_Reverse__c != null && objectOLI.Maximum_Liability_for_Reverse_Percent__c == null}">
                                        <apex:outputField value="{!objectOLI.Maximum_Liability_for_Reverse__c}"  />
                                    </apex:outputPanel>      
                                    
                                    <!-- When Only % is Present -->        
                                    <apex:outputPanel rendered="{!objectOLI.Maximum_Liability_for_Reverse_Percent__c != null && objectOLI.Maximum_Liability_for_Reverse__c ==null}">
                                        Lower of &nbsp;<apex:outputField value="{!objectOLI.Maximum_Liability_for_Reverse_Percent__c}"  /> of product value
                                    </apex:outputPanel>
                                    
                                    </center>
                                </td>
                            </apex:outputPanel>
                            
                            <!-- Display Blank when both are undefined -->
                            <apex:outputPanel rendered="{!objectOLI.Maximum_Liability_for_Reverse_Percent__c == null && objectOLI.Maximum_Liability_for_Reverse__c == null}">
                                <td colspan="5"/>
                            </apex:outputPanel>
                            
                        </tr>
                        
                        
                        <tr>
                            <th id="heading">COD Remittance</th>
                            <td colspan="8">
                                <apex:outputLabel rendered="{!objectOLI.COD_Remittance__c=='Weekly'}">
                                    {!objectOLI.COD_Remittance__c}(Fri)
                                </apex:outputLabel>
                                <apex:outputLabel rendered="{!objectOLI.COD_Remittance__c=='Bi-weekly'}">
                                    {!objectOLI.COD_Remittance__c}(Tue,Fri)
                                </apex:outputLabel>
                                <apex:outputLabel rendered="{!objectOLI.COD_Remittance__c=='Tri-weekly'}">
                                    {!objectOLI.COD_Remittance__c}(Mon,Wed,Fri)
                                </apex:outputLabel>
                                <apex:outputLabel rendered="{!objectOLI.COD_Remittance__c=='Daily'}">
                                    {!objectOLI.COD_Remittance__c}(Mon,Tue,Wed,Thu,Fri)
                                </apex:outputLabel>
                            </td>
                        </tr>
                    </table>
                    
                    <apex:pageBlockSection >
                        <apex:pageBlockSectionItem >
                            <apex:repeat value="{!opp.OpportunityLineItems}" var="oli">
                                <apex:outputField value="{!oli.Description__c}" ></apex:outputField>
                            </apex:repeat>
                        </apex:pageBlockSectionItem> 
                    </apex:pageBlockSection>
                    
                </apex:pageBlock>
            </apex:form>
            
            
            <apex:form rendered="{!IF(AND(Opportunity.RecordType.DeveloperName=='Express_Shipping',slabList.size > 0,opp.OpportunityLineItems.size>0,opp.OpportunityLineItems[0].isCustom__c ),true,false )}">
                <apex:pageBlock id="CustomExpress" >
                    <apex:pageMessages ></apex:pageMessages>
                    <table cellspacing="10" cellpadding="10" border="1" id="CustomExpress" >
                        <tr><td colspan="8" id="heading" ><center><p>Delhivery Commercials - EXPRESS</p></center></td></tr> 
                        
                        <tr>
                            
                            <th id="heading" colspan="3"><center>Weight Slabs</center></th>
                            
                            <th id="heading" ><center>Within-city</center></th>
                            <th id="heading" > <center>Regional (Single <br/>Connection And <br/> Less than 500 Kms)</center></th>
                            <th id="heading" ><center> Metro-Metro </center></th>
                            <th id="heading"><center> Rest of India </center></th>
                            <th id="heading"><center>North East, HP,<br/> Port Blair,J And K </center></th>
                        </tr>
                        
                        <tr>
                            <th id="heading"><center>START RANGE</center></th>
                            <th id="heading"><center>END RANGE</center></th>
                            <th id="heading"><center>EACH SLAB<br/> IN GRAMS</center></th>
                            <th id="heading"><center>ZONE A</center></th>
                            <th id="heading"><center>ZONE B </center></th>
                            <th id="heading"><center>ZONE C</center></th>
                            <th id="heading"><center>ZONE D</center></th>
                            <th id="heading"><center>ZONE E</center></th>
                        </tr>
                        
                        <tr>
                            <th id="heading" colspan="8">Forward</th>
                        </tr>
                        
                        <apex:outputPanel rendered="{!BaseList.size > 0}" >
                            <apex:repeat value="{!BaseList}" var="base" >
                                <tr>
                                    <td><apex:outputField value="{!base.Start_Range__c}"   /></td> 
                                    <td>
                                        <apex:outputField value="{!base.End_Range__c}" rendered="{!if(base.End_Range__c!=null,true,false)}"/>
                                        <apex:outputLabel rendered="{!if(base.End_Range__c==null,true,false)}">
                                            <center>
                                            </center>
                                        </apex:outputLabel>
                                    </td>
                                    <td><apex:outputField value="{!base.Slab__c}"   /></td>
                                    <td><apex:outputField value="{!base.Zone_A_Custom_Price__c}"   /></td>
                                    <td><apex:outputField value="{!base.Zone_B_Custom_Price__c}"    /></td>
                                    <td><apex:outputField value="{!base.Zone_C_Custom_Price__c}"    /></td>
                                    <td><apex:outputField value="{!base.Zone_D_Custom_Price__c}"    /></td>
                                    <td><apex:outputField value="{!base.Zone_E_Custom_Price__c}"    /></td>
                                </tr>
                            </apex:repeat>                
                        </apex:outputPanel>
                        
                        <apex:outputPanel rendered="{!BaseList.size == 0 }" >
                            <tr><td colspan="8" >No Records </td> </tr>
                        </apex:outputPanel>
                        
                        <tr>
                            <th id="heading" colspan="8" >RTO</th>
                        </tr>
                        
                        <apex:outputPanel rendered="{!RTOList.size > 0}" >
                            <apex:repeat value="{!RTOList}" var="RTO" >
                                <tr>
                                    <td><apex:outputField value="{!RTO.Start_Range__c}"   /></td> 
                                    <td>
                                        <apex:outputField value="{!RTO.End_Range__c}" rendered="{!if(RTO.End_Range__c!=null,true,false)}"/>
                                        <apex:outputLabel rendered="{!if(RTO.End_Range__c==null,true,false)}">
                                            <center>
                                            </center>
                                        </apex:outputLabel>
                                    </td>
                                    <td><apex:outputField value="{!RTO.Slab__c}"   /></td>
                                    <td><apex:outputField value="{!RTO.Zone_A_Custom_Price__c}"   /></td>
                                    <td><apex:outputField value="{!RTO.Zone_B_Custom_Price__c}"    /></td>
                                    <td><apex:outputField value="{!RTO.Zone_C_Custom_Price__c}"    /></td>
                                    <td><apex:outputField value="{!RTO.Zone_D_Custom_Price__c}"    /></td>
                                    <td><apex:outputField value="{!RTO.Zone_E_Custom_Price__c}"    /></td>
                                </tr>
                            </apex:repeat>                
                        </apex:outputPanel>     
                        
                        <apex:outputPanel rendered="{!RTOList.size == 0 }" >
                            <tr><td colspan="8" >No Records </td> </tr>
                        </apex:outputPanel>
                        
                        <tr>
                            <th id="heading" colspan="8" >DTO</th>
