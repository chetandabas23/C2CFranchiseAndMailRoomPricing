                        </tr>
                        
                        <!-- <tr>
<th id="heading" colspan="2">GST</th>
<td colspan="8"><apex:outputField value="{!opp.OpportunityLineItems[0].Service_Charges__c}"    /></td>
</tr> -->
                        <tr>
                            <th   colspan="2">GST</th>
                            <td colspan="8"><center>As Applicable</center></td>
                        </tr>
                        
                        
                        <tr>
                                    <th   colspan="2" >Fuel Surcharge (FSC)</th>
                                    <apex:outputPanel rendered="{!opp.OpportunityLineItems[0].Fuel_Surcharge__c!=null }">
                                        
                                        <td colspan="8"><center><apex:outputField value="{!opp.OpportunityLineItems[0].Fuel_Surcharge__c}"  />&nbsp;on Freight Charges</center></td>
                                    </apex:outputPanel>
                                    
                                    <apex:outputPanel rendered="{!opp.OpportunityLineItems[0].Fuel_Surcharge__c==null }">
                                        <td colspan="8"/>
                                        
                                    </apex:outputPanel>
                                </tr>
                        
                        <tr>
                            <th   colspan="2">Maximum Liability for Forward</th>
                            <td colspan="8"><center><apex:outputField value="{!opp.OpportunityLineItems[0].Maximum_liability_per_consignment__c}"    /></center></td>
                        </tr>
                        
                        <tr>
                            <th   colspan="2" >Maximum Liability for Reverse </th>
                            <apex:outputPanel rendered="{!opp.OpportunityLineItems[0].Maximum_Liability_for_Reverse__c != null || opp.OpportunityLineItems[0].Maximum_Liability_for_Reverse_Percent__c != null }"> 
                                <td colspan="8"><center>
                                    <!-- When Both % and Value is Present -->
                                    <apex:outputPanel rendered="{!opp.OpportunityLineItems[0].Maximum_Liability_for_Reverse_Percent__c != null && opp.OpportunityLineItems[0].Maximum_Liability_for_Reverse__c !=null}">
                                        Lower of &nbsp;<apex:outputField value="{!opp.OpportunityLineItems[0].Maximum_Liability_for_Reverse_Percent__c}"  />
                                        of product value or &nbsp;<apex:outputField value="{!opp.OpportunityLineItems[0].Maximum_Liability_for_Reverse__c}"  />
                                    </apex:outputPanel>
                                    
                                    <!-- When Only Value is Present -->
                                    <apex:outputPanel rendered="{!opp.OpportunityLineItems[0].Maximum_Liability_for_Reverse__c != null && opp.OpportunityLineItems[0].Maximum_Liability_for_Reverse_Percent__c == null}">
                                        <apex:outputField value="{!opp.OpportunityLineItems[0].Maximum_Liability_for_Reverse__c}"  />
                                    </apex:outputPanel>      
                                    
                                    <!-- When Only % is Present -->        
                                    <apex:outputPanel rendered="{!opp.OpportunityLineItems[0].Maximum_Liability_for_Reverse_Percent__c != null && opp.OpportunityLineItems[0].Maximum_Liability_for_Reverse__c ==null}">
                                        Lower of &nbsp;<apex:outputField value="{!opp.OpportunityLineItems[0].Maximum_Liability_for_Reverse_Percent__c}"  /> of product value
                                    </apex:outputPanel>
                                    
                                    </center>
                                </td>
                            </apex:outputPanel>
                            
                            <!-- Display Blank when both are undefined -->
                            <apex:outputPanel rendered="{!opp.OpportunityLineItems[0].Maximum_Liability_for_Reverse_Percent__c == null && opp.OpportunityLineItems[0].Maximum_Liability_for_Reverse__c == null}">
                                <td colspan="8"/>
                            </apex:outputPanel>
                            
                        </tr>
                        
                        <tr>
                            <th   colspan="2">COD Remittance</th>
                            <td colspan="8"><center>
                                <apex:outputLabel rendered="{!opp.OpportunityLineItems[0].COD_Remittance__c=='Weekly'}">
                                    {!opp.OpportunityLineItems[0].COD_Remittance__c}(Fri)
                                </apex:outputLabel>
                                <apex:outputLabel rendered="{!opp.OpportunityLineItems[0].COD_Remittance__c=='Bi-weekly'}">
                                    {!opp.OpportunityLineItems[0].COD_Remittance__c}(Tue,Fri)
                                </apex:outputLabel>
                                <apex:outputLabel rendered="{!opp.OpportunityLineItems[0].COD_Remittance__c=='Tri-weekly'}">
                                    {!opp.OpportunityLineItems[0].COD_Remittance__c}(Mon,Wed,Fri)
                                </apex:outputLabel>
                                <apex:outputLabel rendered="{!opp.OpportunityLineItems[0].COD_Remittance__c=='Daily'}">
                                    {!opp.OpportunityLineItems[0].COD_Remittance__c}(Mon,Tue,Wed,Thu,Fri)
                                </apex:outputLabel>
                                </center>
                            </td>
                        </tr>  
                        
                        
                        
                    </table>
                    
                    <apex:pageBlockSection >
                        <apex:pageBlockSectionItem >
                            <apex:repeat value="{!opp.OpportunityLineItems}" var="oli">
                                <apex:outputText value="{!oli.Description__c}" style="font-size:12px;"></apex:outputText>
                            </apex:repeat>
                        </apex:pageBlockSectionItem> 
                    </apex:pageBlockSection>
                </apex:pageBlock>
                
                <br/> 
                <br/> 
                <br/> 
                <br/> 
                <br/> 
                
            </div>
        </apex:form>
    </body>
